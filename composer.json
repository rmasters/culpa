{
    "name": "rmasters/culpa",
    "description": "Adds Blameable support to Eloquent models in Laravel",
    "keywords": ["laravel", "blameable", "created-by"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ross Masters",
            "email": "ross@rossmasters.com"
        },
        {
            "name": "Nico Stapelbroek",
            "email": "nstapelbroek@gmail.com"
        }
    ],
    "homepage": "https://github.com/rmasters/culpa",
    "support": {
        "issues": "https://github.com/rmasters/culpa/issues",
        "wiki": "https://github.com/rmasters/culpa/wiki"
    },
    "require": {
        "illuminate/support": "5.1.*",
        "illuminate/config": "5.1.*",
        "illuminate/events": "5.1.*"
    },
    "require-dev": {
        "ext-sqlite3": "*",
        "illuminate/database": "5.1.*",
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "0.9.*"
    },
    "autoload": {
        "psr-4": {
            "Culpa\\": "src/",
            "Culpa\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "config": {
        "preferred-install": "dist"
    }
}
